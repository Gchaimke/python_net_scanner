    "version": "0.2.0",
    "configurations": [
        {"name": "Run with range",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/net_scanner.py",
            "console": "integratedTerminal",
            "args": [
                "-s",
                "192.168.1.1",
                "-e",
                "192.168.1.5"
            ]
        },
        {
            "name": "Run with one IP",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/net_scanner.py",
            "console": "integratedTerminal",
            "args": [
                "-s",
                "192.168.1.1"
            ]
        }
    ]
}